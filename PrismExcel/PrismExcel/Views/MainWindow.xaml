<Window
    x:Class="PrismExcel.Views.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:core="clr-namespace:PrismExcel.Core;assembly=PrismExcel.Core"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:prism="http://prismlibrary.com/"
    xmlns:viewmodels="clr-namespace:PrismExcel.ViewModels"
    Title="DataGrid like Excel (t13801206 - 2021.10)"
    Width="500"
    Height="500"
    d:DataContext="{d:DesignInstance Type=viewmodels:MainWindowViewModel}"
    prism:ViewModelLocator.AutoWireViewModel="True"
    mc:Ignorable="d">
    <Grid>
        <ContentControl prism:RegionManager.RegionName="{x:Static core:RegionNames.MainTableRegion}" />
    </Grid>
</Window>
